.search-wrap{
    padding: 11rpx 30rpx 0;
}
.search-bar-wrap{
    width: 100%;
    height: 80rpx;
    background: #F3F3F3;
    border-radius: 40rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx;
    box-sizing: border-box;
}
.search-bar{
    font-family: PingFang-SC-Medium;
    font-size: 28rpx;
    color: #999999;
}
.icon_search{
    width: 34rpx;
    height: 32rpx;
}
.ui-list{
    margin-top: 13rpx;
}
